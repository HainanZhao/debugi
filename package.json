{
  "name": "debugi",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run lint && ng build && npm run seo:generate",
    "build:prod": "npm run lint && ng build --configuration production && npm run prerender:advanced && npm run seo:generate",
    "build:gh-pages": "npm run build:prod",
    "build:no-lint": "ng build && npm run seo:generate",
    "build:prod:no-lint": "ng build --configuration production && npm run seo:generate",
    "watch": "ng build --watch --configuration development",
    "test": "npm run lint && ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/app/**/*.{ts,html,css,scss,json}\"",
    "format:check": "prettier --check \"src/app/**/*.{ts,html,css,scss,json}\"",
    "format:lint": "npm run format && npm run lint:fix",
    "seo:generate": "node scripts/generate-seo.js",
    "seo:sitemap": "node scripts/generate-sitemap.js",
    "seo:robots": "node scripts/generate-robots.js",
    "prerender": "node scripts/prerender.js",
    "prerender:advanced": "node scripts/prerender-advanced.js",
    "build:ssg": "npm run lint && ng build --configuration production && npm run prerender && npm run seo:generate"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@types/d3": "^7.4.3",
    "@types/pako": "^2.0.3",
    "@types/qrcode": "^1.5.5",
    "alasql": "^4.6.6",
    "d3": "^7.9.0",
    "mathjs": "^13.0.0",
    "pako": "^2.1.0",
    "phosphor-icons": "^1.4.2",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.13",
    "@angular/cli": "^19.2.13",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "19.6.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "express": "^5.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.10.0",
    "sharp": "^0.34.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.32.1"
  }
}
